{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python@4.0.0"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/",
      "dest": "index.html"
    }
  ],
  "functions": {
    "api/main.py": {
      "maxDuration": 10
    }
  }
}