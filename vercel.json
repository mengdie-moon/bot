{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    },
    {
      "src": "/",
      "dest": "index.html"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.10"
  },
  "functions": {
    "main.py": {
      "maxDuration": 10
    }
  }
}