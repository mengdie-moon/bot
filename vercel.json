{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python@latest"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "napcat/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/webui/(.*)",
      "dest": "/napcat/static/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/",
      "dest": "index.html"
    }
  ]
}