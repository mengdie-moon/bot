{
  "version": 2,
  "runtime": "python3.11",
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/",
      "dest": "index.html"
    }
  ],
  "installCommand": "pip install -r requirements.txt",
  "buildCommand": "echo 'Building...'"
}